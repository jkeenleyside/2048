[project]
	description = A small clone of 1024 (https://play.google.com/store/apps/details?id=com.veewo.a1024)
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/John Keenleyside (jkeenleyside)
[access "refs/for/refs/*"]
	read = group user/John Keenleyside (jkeenleyside)
	push = group user/John Keenleyside (jkeenleyside)
	label-Code-Review = -2..+2 group user/John Keenleyside (jkeenleyside)
	label-Verified = -1..+1 group user/John Keenleyside (jkeenleyside)
	rebase = group user/John Keenleyside (jkeenleyside)
	removeReviewer = group user/John Keenleyside (jkeenleyside)
	submit = group user/John Keenleyside (jkeenleyside)
[access "refs/heads/*"]
	read = group user/John Keenleyside (jkeenleyside)
	create = group user/John Keenleyside (jkeenleyside)
	push = +force group user/John Keenleyside (jkeenleyside)
	pushMerge = group user/John Keenleyside (jkeenleyside)
[access "refs/tags/*"]
	push = +force group user/John Keenleyside (jkeenleyside)
	pushTag = group user/John Keenleyside (jkeenleyside)
	pushSignedTag = group user/John Keenleyside (jkeenleyside)
